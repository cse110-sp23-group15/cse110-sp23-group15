{
  "plugins": [
    "plugins/markdown"
  ],
  "source": {
    "include": [
      "./src/js/"
    ],
    "includePattern": ".+\\.js(doc|x)?$",
    "excludePattern": "(node_modules/|docs)"
  },
  "templates": {
    "cleverLinks": true,
    "monospaceLinks": true
  },
  "opts": {
    "destination": "./docs",
    "recurse": true,
    "readme": "./README.md"
  }
}
