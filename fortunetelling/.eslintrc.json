{
	"env": {
		"browser": true,
		"es2021": true,
		"node": true
	},
	"extends": [
		"google",
		"plugin:prettier/recommended",
		"plugin:css/recommended",
		"plugin:jest/recommended"
	],
	"plugins": ["html", "css", "jest"],
	"overrides": [],
	"parserOptions": {
		"ecmaVersion": "latest",
		"sourceType": "module"
	},
	"rules": {
		"no-unused-vars": "warn",
		"no-console": "off"
	}
}
