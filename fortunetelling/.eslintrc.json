{
	"env": {
		"browser": true,
		"es2021": true,
		"node": true
	},
	"extends": [
		"google",
		"plugin:prettier/recommended",
		"plugin:jest/recommended",
		"plugin:import/recommended"
	],
	"overrides": [],
	"parser": "@babel/eslint-parser",
	"parserOptions": {
		"ecmaVersion": "6",
		"sourceType": "module",
		"requireConfigFile": false,
		"babelOptions": {
			"plugins": ["@babel/plugin-syntax-import-assertions"]
		}
	},
	"rules": {
		"no-unused-vars": "warn",
		"no-console": "off"
	}
}
