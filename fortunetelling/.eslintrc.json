{
	"env": {
		"browser": true,
		"es2021": true,
		"node": true
	},
	"extends": ["google", "plugin:prettier/recommended", "plugin:css/standard"],
	"plugins": ["html", "css"],
	"overrides": [],
	"parserOptions": {
		"ecmaVersion": "latest",
		"sourceType": "module"
	},
	"rules": {
		"no-unused-vars": "warn",
		"no-console": "off"
	}
}
