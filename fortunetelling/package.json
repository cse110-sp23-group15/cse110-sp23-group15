{
  "name": "fortunetelling",
  "version": "1.0.0",
  "description": "## Development",
  "main": "main.js",
  "scripts": {
    "prepare": "cd .. && npx husky install fortunetelling/.husky",
    "test": "jest",
    "format": "npm run fix:prettier && npm run fix:eslint",
    "docgen": "npx jsdoc -c jsdoc.json",
    "dry:eslint": "npx eslint .",
    "fix:eslint": "npx eslint . --fix",
    "fix:prettier": "npx prettier --write './**/*.{html,css,js}'"
  },
  "author": "",
  "license": "ISC",
  "devDependencies": {
    "@babel/core": "^7.21.8",
    "@babel/preset-env": "^7.21.5",
    "babel-jest": "^29.5.0",
    "eslint": "^8.40.0",
    "eslint-config-google": "^0.14.0",
    "eslint-config-prettier": "^8.8.0",
    "eslint-plugin-css": "^0.8.0",
    "eslint-plugin-html": "^7.1.0",
    "eslint-plugin-jest": "^27.2.1",
    "eslint-plugin-prettier": "^4.2.1",
    "husky": "^8.0.3",
    "jest": "^29.5.0",
    "jest-puppeteer": "^8.0.6",
    "jsdoc": "^4.0.2",
    "lint-staged": "^13.2.2",
    "puppeteer": "^20.2.1",
    "webdriverio": "^8.10.2"
  },
  "jest": {
    "preset": "jest-puppeteer",
    "verbose": true
  },
  "lint-staged": {
    "*.{html,css,js}": "prettier --write",
    "*.{html,js}": "eslint --fix"
  },
  "type": "module",
  "dependencies": {
    "expect": "^29.5.0",
    "expect-webdriverio": "^4.2.3"
  }
}
